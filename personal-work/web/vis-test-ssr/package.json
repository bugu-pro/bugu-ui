{
  "private": true,
  "scripts": {
    "test": "umi test",
    "build": "umi build",
    "precommit": "lint-staged",
    "start": "npm run build && nodemon server.js",
    "lint": "eslint {src,mock,tests}/**/*.{ts,tsx} --fix",
    "debug": "cross-env RM_TMPDIR=none COMPRESS=none UMI_ENV=prod umi build && node server.js",
    "dev": "cross-env UMI_UI=none NODE_ENV=development concurrently \"umi dev\" \"nodemon server.js\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^3.19.5",
    "aos": "^2.3.4",
    "cross-env": "^5.2.0",
    "dva": "^2.6.0-beta.6",
    "koa": "^2.7.0",
    "koa-compress": "^3.0.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-side-effect": "^2.1.0",
    "regenerator-runtime": "^0.13.2",
    "umi-request": "^1.2.4",
    "umi-server": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "umi": "^2.9.0",
    "umi-plugin-react": "^1.8.0",
    "umi-plugin-gh-pages": "^0.1.0",
    "umi-types": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
